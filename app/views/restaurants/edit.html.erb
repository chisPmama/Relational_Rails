<%= form_with url: "/restaurants/#{@restaurant.id}", method: :patch, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <br><br>
  <%= form.label :location %>
  <%= form.text_field :location %>
  <br><br>
   <%= form.label :offers_insurance %>
  <%= form.text_field :offers_insurance %>
  <br><br>
   <%= form.label :max_employee_quantity %>
  <%= form.text_field :max_employee_quantity %>
  <br><br>
   <%= form.label :opening_date %>
  <%= form.text_field :opening_date %>
<br><br>
  <%= form.submit "Update Restaurant" %>
<% end %>
